{
  "name": "sujin",
  "version": "9.0.0",
  "description": "Make mobile apps, web, and desktop apps with one React code",
  "scripts": {
    "arson": "npm run test && npm run lint && npm run phpunit && npm run phpcs",
    "build": "cross-env BABEL_ENV=default webpack",
    "bundle-analyzer": "npm run run:stats && npm run run:bundle-analyzer",
    "lint": "eslint app/**/*.ts app/**/*.tsx app/**/*.js app/**/*.jsx",
    "phpcbf": "vendor/bin/phpcbf --standard=./.configs/phpcs.xml -s && true",
    "phpcs": "vendor/bin/phpcs --standard=./.configs/phpcs.xml -s",
    "phpdoc": "vendor/bin/phpdoc -d ./classes -t ./logs/phpdoc",
    "phpunit": "vendor/bin/phpunit -c ./.configs/phpunit/phpunit.xml",
    "run:bundle-analyzer": "NODE_ENV=production webpack-bundle-analyzer logs/stats.json",
    "run:stats": "NODE_ENV=production webpack --profile --json > logs/stats.json",
    "schema:build": "./scripts/build-schema.sh",
    "start": "cross-env BABEL_ENV=default webpack --watch",
    "test": "jest --coverage",
    "test:dev": "jest"
  },
  "author": {
    "email": "sujin.2f@google.com",
    "name": "Sujin 수진 Choi",
    "url": "http://sujinc.com"
  },
  "bugs": {
    "url": "https://github.com/sujin2f/SUjin/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sujin2f/SUjin.git"
  },
  "private": false,
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.19.0",
    "foundation-sites": "^6.5.3",
    "history": "^4.9.0",
    "object-hash": "^1.3.1",
    "path-to-regexp": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.4.4",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "@wordpress/compose": "^3.3.0",
    "@wordpress/data": "^4.5.0",
    "@wordpress/element": "^2.4.0",
    "@wordpress/eslint-plugin": "^3.1.0",
    "@wordpress/shortcode": "^2.3.0",
    "@wordpress/url": "^2.6.0",
    "ansi-colors": "^2.0.1",
    "autoprefixer": "^9.6.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "clean-webpack-plugin": "^2.0.2",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^2.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fancy-log": "^1.3.2",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rememo": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "tar": ">=4.4.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-clean": "^1.2.3",
    "webpack-cli": "^3.3.2",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.1"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "app/(.*)": "<rootDir>/app/$1",
      "\\.(jpg|jpeg|png|gif|svg|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.configs/jest/__mocks__/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "app"
    ],
    "globals": {
      "wp": {}
    },
    "collectCoverageFrom": [
      "app/**",
      "!app/**/*.spec.*"
    ],
    "collectCoverage": true,
    "coverageDirectory": "<rootDir>/logs",
    "coverageReporters": [
      "clover"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/vendor/"
    ],
    "setupFiles": [
      "<rootDir>/.configs/jest/global.js"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "plugin:@wordpress/eslint-plugin/esnext",
      "plugin:@typescript-eslint/recommended"
    ],
    "env": {
      "browser": false,
      "es6": true,
      "node": true,
      "jest/globals": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "globals": {
      "sujin": true,
      "wp": true,
      "window": true,
      "document": true
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import",
      "@typescript-eslint",
      "jest"
    ],
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.babel.js"
        }
      }
    },
    "rules": {
      "template-curly-spacing": [
        "error",
        "never"
      ],
      "no-console": "off",
      "react/react-in-jsx-scope": "off",
      "operator-linebreak": [
        "error",
        "after"
      ],
      "max-len": [
        "error",
        150,
        2
      ],
      "click-events-have-key-events": "off",
      "react/prop-types": "off",
      "react/prefer-stateless-function": "off",
      "import/no-extraneous-dependencies": "off",
      "class-methods-use-this": "off",
      "react/destructuring-assignment": "off",
      "react/no-danger": "off",
      "indent": [
        "error",
        2,
        {
          "SwitchCase": 1
        }
      ],
      "space-in-parens": [
        "error",
        "never"
      ],
      "computed-property-spacing": [
        "error",
        "never"
      ],
      "space-unary-ops": [
        "error",
        {
          "words": true
        }
      ],
      "nonwords": 0,
      "array-bracket-spacing": [
        "error",
        "never"
      ],
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".jsx",
            ".tsx"
          ]
        }
      ],
      "react/no-string-refs": "off",
      "lines-between-class-members": "off",
      "import/no-cycle": "off",
      "@typescript-eslint/no-inferrable-types": "off",
      "import/no-named-default": "off",
      "@typescript-eslint/interface-name-prefix": "off",
      "import/prefer-default-export": "off"
    },
    "overrides": [
      {
        "files": [
          "*.js",
          "*.jsx"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": "off"
        }
      },
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "rules": {
          "jsx-a11y/control-has-associated-label": "off",
          "@typescript-eslint/ban-ts-ignore": "off"
        }
      }
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript"
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "wp.element.createElement"
        }
      ],
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
