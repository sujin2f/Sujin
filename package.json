{
  "name": "chain-react",
  "version": "2.0.0",
  "description": "Make mobile apps, web, and desktop apps with one React code",
  "scripts": {
    "bundle-analyzer": "npm run run:stats && npm run run:bundle-analyzer",
    "ssr:dev": "NODE_ENV=development webpack --watch --progress & NODE_ENV=development nodemon src/ssr/server.js",
    "ssr:build": "NODE_ENV=production webpack --progress & NODE_ENV=production nodemon src/ssr/server.js",
    "start": "NODE_ENV=development webpack-dev-server --open --hot",
    "run:bundle-analyzer": "NODE_ENV=production webpack-bundle-analyzer stats.json",
    "run:stats": "NODE_ENV=production webpack --profile --json > stats.json",
    "precommit": "eslint src --quiet"
  },
  "author": {
    "email": "sujin.2f@google.com",
    "name": "Sujin Choi",
    "url": "http://sujinc.com"
  },
  "bugs": {
    "url": "https://github.com/sujin2f/chain-react/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "flow",
    "sass",
    "webpack",
    "hot",
    "reload",
    "android",
    "phonegap"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sujin2f/chain-react.git"
  },
  "private": false,
  "license": {
    "type": "Apache-2.0",
    "url": "https://opensource.org/licenses/apache2.0.php"
  },
  "config": {
    "productName": "Chain React",
    "appId": "com.sujinc.chaninreact",
    "paths": {
      "app": "./app",
      "src": "./src",
      "output": "./build",
      "templates": "./templates"
    }
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "config": "^1.30.0",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
