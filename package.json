{
    "name": "sujin",
    "version": "11.0.0",
    "description": "WordPress theme for sujinc.com",
    "license": "MIT",
    "main": ".build/production/server/index.js",
    "scripts": {
        "dev": "npm-run-all -s dev:clean dev:watch",
        "dev:clean": "rimraf .build/development/*",
        "dev:watch": "concurrently --kill-others-on-fail \"yarn dev:node\" \"yarn dev:react\"",
        "dev:node": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn ./src/server/index.ts",
        "dev:react": "NODE_ENV=development webpack --watch",
        "stage": "npm-run-all -s stage:clean stage:tsc stage:webpack",
        "stage:clean": "rimraf .build/stage/*",
        "stage:webpack": "NODE_ENV=stage webpack",
        "stage:tsc": "NODE_ENV=stage tsc --project tsconfig.stage.json",
        "stage:server": "NODE_ENV=stage node .build/stage/server/index.js",
        "stage:analyze": "source-map-explorer '.build/stage/frontend/static/js/*.js'",
        "prod": "npm-run-all -s prod:clean prod:tsc prod:webpack",
        "prod:clean": "rimraf .build/production/*",
        "prod:webpack": "NODE_ENV=production webpack",
        "prod:tsc": "NODE_ENV=production tsc --project tsconfig.production.json",
        "prod:server": "NODE_ENV=production node .build/production/server/index.js",
        "prod:analyze": "source-map-explorer '.build/production/frontend/static/js/*.js'",
        "test": "NODE_ENV=jest jest --runInBand"
    },
    "dependencies": {},
    "devDependencies": {
        "@apollo/client": "^3.6.9",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.3.0",
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.13",
        "@types/jest": "^28.1.6",
        "@types/module-alias": "^2.0.1",
        "@types/mysql": "^2.15.21",
        "@types/node": "^18.7.3",
        "@types/react": "^18.0.17",
        "@types/react-color": "^3.0.6",
        "@types/react-dom": "^18.0.6",
        "@types/react-router-dom": "^5.3.3",
        "@types/supertest": "^2.0.12",
        "axios": "^0.27.2",
        "concurrently": "^7.3.0",
        "dotenv": "^16.0.1",
        "ejs": "^3.1.8",
        "express": "^4.18.1",
        "express-graphql": "^0.12.0",
        "foundation-sites": "^6.7.5",
        "graphql": "^16.5.0",
        "jest": "^28.1.3",
        "module-alias": "^2.2.2",
        "mysql": "^2.18.1",
        "node-cache": "^5.1.2",
        "node-sass": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "path": "^0.12.7",
        "path-to-regexp": "^6.2.1",
        "php-unserialize": "^0.0.1",
        "promise-mysql": "^5.2.0",
        "react": "^18.2.0",
        "react-color": "^2.19.3",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "react-scripts": "^5.0.1",
        "rimraf": "^3.0.2",
        "source-map-explorer": "^2.5.2",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.1.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "4.7.4",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
