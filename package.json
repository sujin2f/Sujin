{
    "name": "sujin",
    "version": "10.0.0",
    "description": "WordPress theme for sujinc.com",
    "license": "MIT",
    "main": ".build/production/server/index.js",
    "scripts": {
        "dev": "npm-run-all -s dev:clean dev:watch",
        "dev:clean": "rimraf .build/development/*",
        "dev:watch": "concurrently --kill-others-on-fail \"yarn dev:node\" \"yarn dev:react\"",
        "dev:node": "NODE_ENV=development ts-node-dev --compiler ttypescript --respawn ./src/server/index.ts",
        "dev:react": "NODE_ENV=development webpack --watch",
        "stage": "npm-run-all -s stage:clean stage:tsc stage:webpack",
        "stage:clean": "rimraf .build/stage/*",
        "stage:webpack": "NODE_ENV=stage webpack",
        "stage:tsc": "NODE_ENV=stage tsc --project tsconfig.stage.json",
        "stage:server": "NODE_ENV=stage node .build/stage/server/index.js",
        "prod:build": "npm-run-all -s prod:clean prod:tsc prod:webpack",
        "prod:clean": "rimraf .build/production/*",
        "prod:webpack": "NODE_ENV=production webpack",
        "prod:tsc": "NODE_ENV=production tsc --project tsconfig.production.json",
        "prod:server": "NODE_ENV=production node .build/production/server/index.js",
        "test": "NODE_ENV=jest jest --runInBand"
    },
    "dependencies": {
        "axios": "^0.24.0",
        "dotenv": "^8.2.0",
        "ejs": "^3.1.6",
        "express": "^4.17.1",
        "express-graphql": "^0.12.0",
        "graphql": "^15.5.0",
        "module-alias": "^2.2.2",
        "mysql": "^2.18.1",
        "node-cache": "^5.1.2",
        "path": "^0.12.7",
        "php-unserialize": "^0.0.1",
        "promise-mysql": "^5.0.3"
    },
    "devDependencies": {
        "@apollo/client": "^3.3.15",
        "@testing-library/jest-dom": "^5.11.0",
        "@testing-library/react": "^12.1.4",
        "@types/ejs": "^3.0.6",
        "@types/express": "^4.17.7",
        "@types/jest": "^26.0.4",
        "@types/module-alias": "^2.0.1",
        "@types/mysql": "^2.15.18",
        "@types/node": "^14.0.22",
        "@types/react": "^16.9.0",
        "@types/react-color": "^3.0.6",
        "@types/react-dom": "^16.9.0",
        "@types/react-router-dom": "^5.1.5",
        "@types/supertest": "^2.0.10",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "concurrently": "^5.2.0",
        "foundation-sites": "^6.6.3",
        "jest": "^26.1.0",
        "node-sass": "^6.0.1",
        "npm-run-all": "^4.1.5",
        "path-to-regexp": "^6.2.0",
        "react": "^16.13.1",
        "react-color": "^2.19.3",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^4.0.3",
        "rimraf": "^3.0.2",
        "supertest": "^4.0.2",
        "ts-jest": "^26.1.3",
        "ts-node": "^10.4.0",
        "ts-node-dev": "^1.0.0-pre.50",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "ttypescript": "^1.5.10",
        "typescript": "^3.9.6",
        "webpack": "^4.42.0",
        "webpack-cli": "^3.3.12"
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    }
}
