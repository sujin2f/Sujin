{
  "$schema": "http://json-schema.org/schema#",
  "$id": "http://sujinc.com/background.schema.json",
  "title": "Background",
  "type": "object",
  "properties": {
    "desktop": {
      "description": "Desktop size image",
      "type": "string",
      "format": "uri",
      "readonly": true
    },
    "mobile": {
      "description": "Mobile size image",
      "type": "string",
      "format": "uri",
      "readonly": true
    },
    "title": {
      "description": "Title of the image",
      "type": "string",
      "readonly": true
    }
  },
  "additionalProperties": false,
  "required": ["desktop", "mobile", "title"]
}
